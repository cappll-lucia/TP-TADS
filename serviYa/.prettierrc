{
	"useTabs": true,
	"singleQuote": true,
	"singleAttributePerLine": true,
	"semi": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-tailwindcss"],
	"pluginSearchDirs": ["."],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
